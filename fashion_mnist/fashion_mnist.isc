isc_project_id = "<project_id>"
experiment_name = "fashion_mnist"
gpu_type = "24GB VRAM GPU"
nnodes = 12
venv_path = "~/.fashion/bin/activate"
output_path = "~/outputs/fashion_mnist"
command = "train.py --epochs 1000 --test-epochs 5 --lr 0.001 --lr-step-epochs 100 --lr-decay-rate 0.8 --batch-size 16 --save-dir $OUTPUT_PATH --tboard-path $OUTPUT_PATH/tb"
